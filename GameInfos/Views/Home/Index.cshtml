@model GameModel
@{
    ViewData["Title"] = "Home Page";
}
<div class="container">
    <div class="row">
        <div class="col-md-6">
            <table class="table table-sm table-striped">
                <thead>
                <tr>
                    <td>Install Count</td>
                    <td>Game Name</td>
                    <td>Company Name</td>
                    <td>Company Email</td>
                    <td>Origin</td>
                </tr>
                </thead>
                <tbody>
                @for (int i = 0; i < Model.GameName.Count ; i++)
                {
                    <tr>
                        <td>@Model.InstallCount[i]</td>
                        <td>@Model.GameName[i]</td>
                        <td>@Model.CompanyName[i]</td>
                        <td>@Model.Email[i]</td>
                        <td>@Model.Country[i]</td>
                    </tr>
                }
    </tbody>
            </table>
        </div>
    </div>
</div>